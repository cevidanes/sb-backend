{
  "dashboard": {
    "uid": "ai-processing",
    "title": "AI Processing",
    "tags": ["ai", "celery", "processing"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 0,
    "refresh": "30s",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
    "panels": [
      {
        "id": 1,
        "title": "Jobs Created vs Completed",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(rate(ai_jobs_created_total[5m])) by (job_type)",
            "legendFormat": "{{job_type}} Created/sec",
            "refId": "A"
          },
          {
            "expr": "sum(rate(ai_jobs_completed_total{status=\"completed\"}[5m])) by (job_type)",
            "legendFormat": "{{job_type}} Completed/sec",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "tooltip": false,
                "viz": false,
                "legend": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            },
            "unit": "ops"
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 2,
        "title": "Jobs Failed by Type",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(increase(ai_jobs_failed_total[24h])) by (job_type)",
            "format": "table",
            "instant": true,
            "legendFormat": "",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto",
              "inspect": false
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 10
                },
                {
                  "color": "red",
                  "value": 50
                }
              ]
            },
            "color": {
              "mode": "thresholds"
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "job_type"
              },
              "properties": [
                {
                  "id": "custom.width",
                  "value": 200
                }
              ]
            }
          ]
        },
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "desc": true,
              "displayName": "Value"
            }
          ]
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {
                "job_type": 0,
                "Value": 1
              },
              "renameByName": {
                "Value": "Failed Jobs (24h)"
              }
            }
          }
        ]
      },
